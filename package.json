{
  "name": "tetribass-client",
  "repository": "https://github.com/NunkuGemu/tetribass-client",
  "license": "MIT",
  "dependencies": {
    "@pixi/react": "^7.1.0",
    "animejs": "^3.2.1",
    "dotenv": "^16.1.3",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
