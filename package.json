  "dependencies": {
    "dotenv": "^16.0.3",
    "next": "13.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "autoprefixer": "^10.4.13",
    "eslint": "8.34.0",
    "eslint-config-next": "13.2.1",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.6",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=18.0.0"
