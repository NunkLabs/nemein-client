    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "autoprefixer": "^10.4.14",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{tsx,ts}": [
      "next lint",
      "prettier --write --ignore-unknown"
    ]
  },
  "scripts": {
    "build": "next build",
    "clean": "rm -rf dist node_modules",
    "dev": "next dev --turbo",
    "format": "pnpm prettier --write app/ libs/",
